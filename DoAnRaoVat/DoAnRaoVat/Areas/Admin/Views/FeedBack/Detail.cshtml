@model Model.EF.FeedBack

@{
    ViewBag.Title = "Detail";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<section id="main-content">
    <section class="wrapper">
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li><i class="fa fa-home"></i><a href="@Url.Action("Index","Home")">Trang chủ</a></li>
                    <li><i class="fa fa-users"></i>Quản lý liên hệ</li>
                    <li>Chi tiết</li>
                </ol>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <section class="panel">
                    <header class="panel-heading">
                        <div class="col-sm-4">
                            <h3><strong>Ý kiến phản hồi người dùng</strong></h3>
                        </div>
                        <div class="button-ds">
                            <a href="@Url.Action("Index","FeedBack")" class="btn btn-success btn-sm">Quay lại</a>
                        </div>
                    </header>
                    <div class="panel-body">
                        <div class="form">

                            <div class="form-group col-lg-6">
                                @Html.LabelFor(model => model.Name, new { @class = "control-label col-md-4" })
                                <div class="col-md-8">
                                    <span>@Model.Name</span>
                                </div>
                            </div>

                            <div class="form-group col-lg-6">
                                @Html.LabelFor(model => model.Phone, new { @class = "control-label col-md-4" })
                                <div class="col-md-8">
                                    <span>@Model.Phone</span>
                                </div>
                            </div>

                            <div class="form-group col-lg-6">
                                @Html.LabelFor(model => model.Email, new { @class = "control-label col-md-4" })
                                <div class="col-md-8">
                                    <span>@Model.Email</span>
                                </div>
                            </div>

                            <div class="form-group col-lg-6">
                                @Html.LabelFor(model => model.Address, new { @class = "control-label col-md-4" })
                                <div class="col-md-8">
                                    <span>@Model.Address</span>
                                </div>
                            </div>

                            <div class="form-group col-lg-12">
                                @Html.LabelFor(model => model.Content, new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    <p>@Model.Content</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </section>
</section>

